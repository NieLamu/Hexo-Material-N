<% if(page.title) { %>
<% var title = page.title + ' ~ ' + config.title %>
<% } else if(is_archive()){ %>
<% var title = 'Archive' + ' ~ ' + config.title %>
<% } else { %>
<% var title = config.title %>
<% } %>

<head>
    <meta charset="utf-8" />
    <meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0, shrink-to-fit=no" name="viewport" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <link rel="apple-touch-icon" sizes="76x76" href="<%- url_for(theme.apple_touch_icon) %>">
    <link rel="icon" type="image/png" href="<%- url_for(theme.favicon) %>">
    <meta name="description" content="<%= config.description %>">
    <meta name="author" content="<%= config.author %>">
    <meta name="keywords" content="<%= config.keywords %>">
    <title><%= title %></title>

    <!--     Fonts and icons     -->
    <script src="https://cdn.bootcss.com/ionicons/4.4.8/ionicons.js"></script>
    <!-- Material Kit CSS -->
    <link href="/css/material-kit.min.css?v=2.0.5" rel="stylesheet" />
    <!-- MyFontsWebfontsKit -->
    <link href="/MyFontsWebfontsKit.css" rel="stylesheet" type="text/css">


</head>